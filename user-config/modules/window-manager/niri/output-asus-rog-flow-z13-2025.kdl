output "eDP-1" {
    mode "2560x1600@180.000"
    scale 1.333333
    transform "normal"
}

output "eDP-2" {
    mode "2560x1600@180.000"
    scale 1.333333
    transform "normal"
}

// NOTE: 1920 is from 2560 / 1.3333 = 1920.
output "DP-1" {
    mode "1920x1080@60.000"
    position x=1920 y=0
}
output "DP-2" {
    mode "1920x1080@60.000"
    position x=1920 y=0
}
output "HDMI-A-1" {
    mode "1920x1080@60.000"
    position x=1920 y=0
}
